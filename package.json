{
  "name": "terrarium",
  "version": "0.1.0",
  "homepage": "./",
  "main": "public/electron.js",
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "start": "BROWSER=none react-app-rewired start",
    "forge-start": "electron-forge start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "electron-dev": "concurrently -k \"npm run watch:css\" \"npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm run build",
    "electron-pack": "build -mw",
    "build:css": "tailwind -i public/styles.css -o src/index.css --minify",
    "watch:css": "TAILWIND_MODE=watch tailwind -i public/styles.css -o src/index.css --watch",
    "package": "electron-forge package",
    "make": "npm run build:css && npm run build && electron-forge make",
    "make:ia32": "npm run make -- --arch ia32",
    "make:universal": "npm run make -- --arch universal",
    "make:x64": "npm run make -- --arch x64",
    "make:arm64": "npm run make -- --arch arm64"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@hookstate/core": "^3.0.13",
    "@hookstate/devtools": "^3.0.2",
    "@iarna/toml": "^2.2.5",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "^5.10.6",
    "@mui/material": "^5.10.7",
    "@reactour/tour": "^3.1.4",
    "@rjsf/core": "^4.2.3",
    "@rjsf/material-ui": "^4.2.3",
    "@terra-money/msg-reader": "^3.0.1",
    "@terra-money/terra.js": "^3.1.7",
    "ansi-to-html": "^0.7.2",
    "bech32": "^2.0.0",
    "dompurify": "^2.4.0",
    "electron-default-menu": "^1.0.2",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.1.0",
    "js-yaml": "^4.1.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.36.1",
    "react-icons": "^4.4.0",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^6.4.1",
    "react-scripts": "^5.0.1",
    "react-virtuoso": "^2.19.0",
    "rsuite-table": "^5.7.1",
    "semver": "^7.3.7",
    "wait-on": "^6.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.66",
    "@types/node": "^16.11.62",
    "@types/react": "^17.0.50",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.12",
    "concurrently": "^6.5.1",
    "electron": "^21.0.0",
    "electron-builder": "^23.3.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.17",
    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/assets/icons/icon.icns",
        "name": "Terrarium"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "terrarium"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}

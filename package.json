{
  "name": "terrarium",
  "version": "0.0.0",
  "homepage": "./",
  "main": "public/electron.js",
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "start": "BROWSER=none react-app-rewired start",
    "forge-start": "electron-forge start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "electron-dev": "concurrently -k \"npm run watch:css\" \"npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm run build",
    "electron-pack": "build -mw",
    "build:css": "tailwind -i public/styles.css -o src/index.css --minify",
    "watch:css": "TAILWIND_MODE=watch tailwind -i public/styles.css -o src/index.css --watch",
    "package": "electron-forge package",
    "make": "npm run build:css && npm run build && electron-forge make"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@hookstate/core": "^3.0.13",
    "@hookstate/devtools": "^3.0.2",
    "@iarna/toml": "^2.2.5",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.6",
    "@rjsf/core": "^4.2.0",
    "@rjsf/material-ui": "^4.2.2",
    "@terra-money/msg-reader": "^3.0.1",
    "@terra-money/terra.js": "^3.1.3",
    "ansi-to-html": "^0.7.2",
    "bech32": "^2.0.0",
    "dompurify": "^2.3.8",
    "electron-default-menu": "^1.0.2",
    "electron-is-dev": "^2.0.0",
    "electron-store": "^8.0.2",
    "js-yaml": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-virtuoso": "^2.14.0",
    "semver": "^7.3.7",
    "wait-on": "^6.0.1"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^18.0.1",
    "concurrently": "^6.5.1",
    "electron": "^15.5.7",
    "electron-builder": "^23.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.3.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/assets/icons/icon.icns",
        "name": "Terrarium"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "terrarium"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}

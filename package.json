{
  "name": "terrarium",
  "version": "0.0.0",
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "start": "npm run watch:css && react-app-rewired start",
    "build": "npm run build:css && react-app-rewired build",
    "test": "react-app-rewired test",
    "electron-dev": "concurrently -k \"BROWSER=none npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "npm run build",
    "electron-pack": "build -mw",
    "build:css": "postcss public/styles.css -o src/index.css",
    "watch:css": "postcss public/styles.css -o src/index.css"
  },
  "dependencies": {
    "@hookstate/core": "^4.0.0-rc.4",
    "@terra-money/msg-reader": "^3.0.1",
    "@terra-money/terra.js": "^3.1.3",
    "ansi-to-react": "^6.1.6",
    "bech32": "^2.0.0",
    "dotenv": "^16.0.1",
    "electron": "^15.5.7",
    "electron-builder": "^23.1.0",
    "electron-is-dev": "^2.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-store": "^8.0.2",
    "js-yaml": "^4.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dynamic-virtual-scroll": "^1.3.2",
    "react-icons": "^4.4.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "react-scroll-to-bottom": "^4.2.0",
    "react-virtuoso": "^2.14.0",
    "wait-on": "^6.0.1"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^18.0.1",
    "@types/react-scroll-to-bottom": "^4.2.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^6.5.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "foreman": "^3.0.1",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.3.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
